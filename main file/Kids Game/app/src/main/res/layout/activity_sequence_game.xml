<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <FrameLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
        <View android:layout_gravity="top|left|center_vertical|center_horizontal|center" android:id="@+id/emiter_center_left" android:layout_width="2dp" android:layout_height="2dp" android:layout_marginLeft="50dp" android:layout_marginTop="20dp"/>
        <View android:layout_gravity="top|right|center_vertical|center_horizontal|center" android:id="@+id/emiter_center_right" android:layout_width="2dp" android:layout_height="2dp" android:layout_marginTop="20dp" android:layout_marginRight="80dp"/>
        <View android:layout_gravity="top|right|center_vertical|center_horizontal|center" android:id="@+id/emiter_top_right" android:layout_width="2dp" android:layout_height="2dp" android:layout_marginTop="20dp" android:layout_marginRight="190dp"/>
        <View android:layout_gravity="top|left|center_vertical|center_horizontal|center" android:id="@+id/emiter_top_left" android:layout_width="2dp" android:layout_height="2dp" android:layout_marginLeft="190dp" android:layout_marginTop="20dp"/>
        <LinearLayout android:gravity="center" android:layout_gravity="center" android:orientation="vertical" android:id="@+id/rlTop" android:background="@drawable/bg_1" android:layout_width="match_parent" android:layout_height="match_parent">
            <LinearLayout android:gravity="left" android:layout_gravity="center" android:orientation="horizontal" android:padding="10dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_weight="4">
                <ImageView android:textSize="40dp" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:id="@+id/home1" android:tag="home" android:background="@drawable/btn_home" android:layout_width="50dp" android:layout_height="50dp" android:layout_weight="0.1"/>
                <LinearLayout android:gravity="center_horizontal" android:layout_gravity="center_horizontal" android:id="@+id/rlBottom" android:layout_width="match_parent" android:layout_height="50dp" android:layout_marginLeft="70dp" android:layout_marginTop="10dp" android:layout_marginRight="70dp" android:layout_marginBottom="10dp" android:layout_weight="2" android:layout_alignParentBottom="true">
                    <TextView android:textSize="20dp" android:textColor="@color/white" android:gravity="center" android:id="@+id/tvTextNumber" android:layout_width="match_parent" android:layout_height="match_parent" android:text="Click in sequence" android:layout_alignParentBottom="true"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:layout_gravity="center" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginBottom="10dp" android:layout_weight="1.1">
                <LinearLayout android:layout_gravity="center" android:orientation="horizontal" android:paddingTop="10dp" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_weight="1">
                    <RelativeLayout android:id="@+id/rl_first" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginLeft="20dp" android:layout_marginRight="40dp">
                        <ImageView android:id="@+id/iv1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/bubble1" android:scaleType="centerInside" android:layout_alignParentLeft="true" android:layout_alignParentRight="true"/>
                        <TextView android:textSize="40dp" android:textStyle="bold" android:textColor="@color/white" android:id="@+id/tv1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:text="1" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/rl_2" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginTop="30dp" android:layout_marginRight="40dp" android:layout_toRightOf="@+id/rl_first">
                        <ImageView android:id="@+id/iv2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/bubblle2" android:scaleType="centerInside" android:layout_alignParentLeft="true" android:layout_alignParentRight="true"/>
                        <TextView android:textSize="40dp" android:textStyle="bold" android:textColor="@color/white" android:id="@+id/tv2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:text="1" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/rl_3" android:layout_width="70dp" android:layout_height="70dp" android:layout_toRightOf="@+id/rl_2">
                        <ImageView android:id="@+id/iv3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/bubble3" android:scaleType="centerInside" android:layout_alignParentLeft="true" android:layout_alignParentRight="true"/>
                        <TextView android:textSize="40dp" android:textStyle="bold" android:textColor="@color/white" android:id="@+id/tv3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:text="1" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/rl_4" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginLeft="50dp" android:layout_marginTop="30dp" android:layout_toRightOf="@+id/rl_3">
                        <ImageView android:id="@+id/iv4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/bubble4" android:scaleType="centerInside" android:layout_alignParentLeft="true" android:layout_alignParentRight="true"/>
                        <TextView android:textSize="40dp" android:textStyle="bold" android:textColor="@color/white" android:id="@+id/tv4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:text="1" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/rl_5" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginLeft="50dp" android:layout_toRightOf="@+id/rl_4">
                        <ImageView android:id="@+id/iv5" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/bubble5" android:scaleType="centerInside" android:layout_alignParentLeft="true" android:layout_alignParentRight="true"/>
                        <TextView android:textSize="40dp" android:textStyle="bold" android:textColor="@color/white" android:id="@+id/tv5" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:text="1" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout android:gravity="top|center_vertical|center_horizontal|center" android:layout_gravity="top|center_vertical|center_horizontal|center" android:orientation="horizontal" android:paddingBottom="10dp" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_weight="1">
                    <RelativeLayout android:id="@+id/rl_6" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginLeft="10dp">
                        <ImageView android:id="@+id/iv6" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/bubble5" android:scaleType="centerInside" android:layout_alignParentLeft="true" android:layout_alignParentRight="true"/>
                        <TextView android:textSize="40dp" android:textStyle="bold" android:textColor="@color/white" android:id="@+id/tv6" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:text="1" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/rl_7" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginLeft="50dp" android:layout_marginTop="30dp" android:layout_toRightOf="@+id/rl_6">
                        <ImageView android:id="@+id/iv7" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/bubble4" android:scaleType="centerInside" android:layout_alignParentLeft="true" android:layout_alignParentRight="true"/>
                        <TextView android:textSize="40dp" android:textStyle="bold" android:textColor="@color/white" android:id="@+id/tv7" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:text="1" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/rl_8" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginLeft="40dp" android:layout_toRightOf="@+id/rl_7">
                        <ImageView android:id="@+id/iv8" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/bubble3" android:scaleType="centerInside" android:layout_alignParentLeft="true" android:layout_alignParentRight="true"/>
                        <TextView android:textSize="40dp" android:textStyle="bold" android:textColor="@color/white" android:id="@+id/tv8" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:text="1" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/rl_9" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginLeft="50dp" android:layout_marginTop="30dp" android:layout_toRightOf="@+id/rl_8">
                        <ImageView android:id="@+id/iv9" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/bubblle2" android:scaleType="centerInside" android:layout_alignParentLeft="true" android:layout_alignParentRight="true"/>
                        <TextView android:textSize="40dp" android:textStyle="bold" android:textColor="@color/white" android:id="@+id/tv9" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:text="1" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/rl_10" android:layout_width="70dp" android:layout_height="70dp" android:layout_marginLeft="50dp" android:layout_toRightOf="@+id/rl_9">
                        <ImageView android:id="@+id/iv10" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/bubble1" android:scaleType="centerInside" android:layout_alignParentLeft="true" android:layout_alignParentRight="true"/>
                        <TextView android:textSize="40dp" android:textStyle="bold" android:textColor="@color/white" android:id="@+id/tv10" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:text="1" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
    <FrameLayout android:gravity="center" android:id="@+id/adViewTop" android:layout_width="match_parent" android:layout_height="wrap_content"/>
</LinearLayout>
